{
  "name": "git-commit-message-plus",
  "displayName": "Git Commit Message Plus",
  "version": "0.0.1",
  "description": "Improved Git commit message editing",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "git",
    "commit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/walles/git-commit-message-plus"
  },
  "license": "MIT",
  "publisher": "walles",
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish",
    "test": "vscode-tmgrammar-test -g tests/support/diff.tmLanguage.json tests/unit/*.test"
  },
  "contributes": {
    "grammars": [
      {
        "language": "git-commit",
        "scopeName": "text.git-commit",
        "path": "./syntaxes/git-commit.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^2.16.0",
    "vscode-tmgrammar-test": "^0.1.1"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "scriptsComment": {
    "package": [
      "From: https://stackoverflow.com/a/67430480/473672"
    ]
  }
}